{
  "manifest_version": 2,
  "name": "LeiCraft_MC Search",
  "version": "1.0.0",
  "author": "LeiCraft_MC",
  "default_locale": "en",
  "description": "LeiCraft_MC Search for Firefox",
  "icons": {
    "48": "searxng48.webp"
  },
  "browser_action": {
    "default_title": "LeiCraft_MC Search"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "LeiCraft_MC Search",
      "keyword": "@lcmc",
      "search_url": "https://search.leicraftmc.de/search?q={searchTerms}",
      "favicon_url": "searxng128.webp",
      "suggest_url": "https://search.leicraftmc.de/autocompleter?q={searchTerms}",
      "is_default": true
    }
  }
}
